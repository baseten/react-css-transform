{
  "name": "react-css-transform",
  "version": "1.1.4",
  "description": "A React component to help handle complex nested 2d and 3d css transformations ",
  "keywords": [
    "React",
    "javascript",
    "typescript",
    "css",
    "transform",
    "2d",
    "3d",
    "matrix",
    "matrices",
    "component"
  ],
  "homepage": "https://github.com/baseten/react-css-transform#readme",
  "bugs": {
    "url": "https://github.com/baseten/react-css-transform/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baseten/react-css-transform.git"
  },
  "license": "ISC",
  "author": "Alex Prokop <alex@baseten.co.uk> (https://baseten.co.uk/)",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./module.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./index.js"
      }
    }
  },
  "main": "index.js",
  "module": "module.js",
  "jsnext:main": "module.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "index.js",
    "module.js"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:dev && npm run build:prod",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "build:prod": "cross-env NODE_ENV=production rollup -c",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf ./dist",
    "lint": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --config .eslintrc.js --ext .mjs,.js,.jsx,.ts,.tsx",
    "lint:eslint:fix": "eslint --config .eslintrc.js --ext .mjs,.js,.jsx,.ts,.tsx --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "prettify": "prettier --write **/**/*.{ts,js,mjs}",
    "pretest": "npm run test:clean && npm run lint",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:ci": "cross-env NODE_ENV=test jest",
    "test:clean": "rimraf ./coverage",
    "test:types": "tsc --noEmit",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "lint-staged": {
    "*.{mjs,js,ts,jsx,tsx}": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "devDependencies": {
    "@cfaester/enzyme-adapter-react-18": "^0.7.0",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-with-typescript": "^30.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gl-matrix": "^3.4.3",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-extended": "^3.2.0",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.2.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.20.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "gl-matrix": ">= 3.0.0",
    "prop-types": ">= 15.0",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  }
}
